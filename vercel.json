{
    "builds": [{
        "src": "GrupoZero/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize" : "15mb", "runtime": "python3.9" }
    }],
    "routes":[
        {
            "src": "core/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "GrupoZero/wsgi.py"
        }
        
    ]
}